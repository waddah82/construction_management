{
 "doctype": "DocType",
  "name": "Expenses",
  "module": "construction_management",
  "custom": 0,
  "istable": 0,
  "editable_grid": 0,
  "autoname": "field:expenses_name",
  "naming_rule": "By fieldname",
  "engine": "InnoDB",
  "track_changes": 1,

  "fields": [
    {
      "fieldname": "expenses_name",
      "label": "Expense Name",
      "fieldtype": "Data",
      "reqd": 1,
      "unique": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "account",
      "label": "Expense Account",
      "fieldtype": "Link",
      "options": "Account",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "remarks",
      "label": "Remarks",
      "fieldtype": "Small Text"
    },
    {
      "fieldname": "company",
      "fieldtype": "Link",
      "label": "Company",
      "options": "Company",
      "remember_last_selected_value": 1
    },
    {
      "fieldname": "enabled",
      "label": "Enabled",
      "fieldtype": "Check",
      "default": "1"
    }
  ],

  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "export": 1,
      "print": 1,
      "share": 1,
      "email": 1
    }
  ]
}
